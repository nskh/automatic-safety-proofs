        LET   f = LAMBDA(x:real):
            COND
                        x <= 0 -> g(x),
                x > 0 -> 0
                    ENDCOND
        IN
        derivable?[(left_open(0))](f) AND
        (FORALL(x:(left_open(0))):deriv[(left_open(0))](f)(x) <= 0) AND
        (f(0)=0) AND
        (EXISTS (x : real) :
        x <= 0 AND
        ((-x + xo + 2 >= 0) AND
        (-x + xo - 2 <= 0) AND
        (yo - f(x) + 1 >= 0) AND
        (yo - f(x) - 1 <= 0)) AND
        xo + 2 >= x AND xo - 2 <= x)
        IMPLIES
    (       (  ((yo - f(xo - 2) - 1 >= 0) AND
            (yo - f(xo + 2) + 1 <= 0)) OR
            ((yo - f(xo - 2) + 1 >= 0) AND
            (yo - f(xo + 2) - 1 <= 0)) OR
            ((yo - f(xo + 2) - 1 >= 0) AND	
            (yo - f(xo - 2) + 1 <= 0)) OR
            ((yo - f(xo + 2) + 1 >= 0) AND
            (yo - f(xo - 2) - 1 <= 0)))
        OR
        (((xo  >= -2 ) AND
        ( xo  <= 2 ) AND
        (yo  >= -1 + f(0)) AND
        (yo  <= 1 + f(0)) )))

%|- descending_case : PROOF
%|- (THEN (SKEEP*) (SKOLETIN*) (FLATTEN) (SKEEP)
%|-  (SPREAD (CASE "xo+2 <= 0")
%|-   ((THEN (LEMMA "mvt_gen_le_lo")
%|-     (SPREAD (INST -1 "f" "0" "0" "xo+2" "x")
%|-      ((SPREAD (SPLIT -1)
%|-        ((THEN (ASSERT) (LEMMA "mvt_gen_le_lo")
