        LET f = LAMBDA(x:real):
        COND
            x < 0 -> g(0),
            x >= 0 -> g(x)
        ENDCOND
    IN
    derivable?[(right_open(0))](f) AND
    (FORALL(x:(right_open(0))): deriv[(right_open(0))](f)(x) >= 0) AND
    (EXISTS (x : real) :
    ((-x + xo + 1 >= 0) AND
     (-x + xo - 1 <= 0) AND
     (yo - f(x) + 1 >= 0) AND
     (yo - f(x) - 1 <= 0)) AND
    x >= 0 AND
    xo + 1 >= x AND xo + -1 <= x)
    IMPLIES
    ((yo - f(xo - 1) - 1 >= 0) AND
     (yo - f(xo + 1) + 1 <= 0)) OR
    ((yo - f(xo - 1) + 1 >= 0) AND
     (yo - f(xo + 1) - 1 <= 0)) OR
    ((yo - f(xo + 1) - 1 >= 0) AND
     (yo - f(xo - 1) + 1 <= 0)) OR
    ((yo - f(xo + 1) + 1 >= 0) AND
     (yo - f(xo - 1) - 1 <= 0))

